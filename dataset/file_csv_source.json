{
	"name": "file_csv_source",
	"properties": {
		"linkedServiceName": {
			"referenceName": "jparlinkedservice",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"SourceContainer": {
				"type": "string"
			},
			"SourceDirectory": {
				"type": "string"
			},
			"SourceFileName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().SourceFileName",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().SourceDirectory",
					"type": "Expression"
				},
				"container": {
					"value": "@dataset().SourceContainer",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"type": "String"
			},
			{
				"type": "String"
			}
		]
	}
}