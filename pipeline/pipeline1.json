{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "load_rest_api",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET"
					},
					"sink": {
						"type": "AzureSqlSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['status']"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['id']"
								},
								"sink": {
									"name": "id",
									"type": "Int16"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['employee_name']"
								},
								"sink": {
									"name": "employee_name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['employee_salary']"
								},
								"sink": {
									"name": "employee_salary",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['employee_age']"
								},
								"sink": {
									"name": "employee_age",
									"type": "Int16"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['profile_image']"
								},
								"sink": {
									"name": "profile_image",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['message']"
								}
							}
						],
						"mapComplexValuesToString": true
					}
				},
				"inputs": [
					{
						"referenceName": "restapi_connection",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "output_db_rest",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}