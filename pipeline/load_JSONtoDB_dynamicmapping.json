{
	"name": "load_JSONtoDB_dynamicmapping",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"disableMetricsCollection": false
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "JSON_target",
						"type": "DatasetReference",
						"parameters": {
							"ContainerName": "test",
							"DirectoryName": "TestDir",
							"FileName": "TestFileEmp.json"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "AzureDB_target",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}